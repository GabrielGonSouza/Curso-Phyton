import os
os.system("cls")

try:
    numero = int(input("Digite um numero inteiro: "))
    resultado = 10 / numero

except ZeroDivisionError:
    print("Erro não é possivel dividir por zerro.")

except ValueError:
    print("Erro: você não digitou um numero valido.")

else: 
    #Executa apenas se nenhuma exceção tiver ocorrido
    print(f"O resultado da divisão é: {resultado}")
finally:
    #Executa sempre, independentemente de erro ou sucesso
    print("Encerrando a operação...")

